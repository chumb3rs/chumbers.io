{
    "name": "chumbers.io",
    "type": "module",
    "version": "1.1.0",
    "description": "My personal blog website",
    "author": "chumb3rs",
    "license": "GPL-3.0-only",
    "repository": {
        "type": "git",
        "url": "https://github.com/chumb3rs/chumbers.io"
    },
    "keywords": [
        "astro",
        "astrojs",
        "astro-template",
        "astro-blog",
        "astro-b"
    ],
    "bugs": {
        "url": "https://github.com/chumb3rs/chumbers.io/issues"
    },
    "scripts": {
        "dev": "npx tinacms dev -c \"astro dev\"",
        "start": "astro dev",
        "build": "astro build",
        "sync": "astro sync",
        "preview": "astro preview",
        "postbuild": "pagefind --site dist",
        "format": "biome format --write",
        "lint": "biome lint --write",
        "lint:fix": "biome check --write",
        "pre-commit": "lint-staged",
        "prepare": "husky install"
    },
    "dependencies": {
        "@astrojs/rss": "4.0.1",
        "@biomejs/biome": "^2.3.2",
        "astro": "4.0.8",
        "astro-font": "^0.0.72"
    },
    "devDependencies": {
        "@astrojs/mdx": "2.0.3",
        "@astrojs/sitemap": "3.0.4",
        "@astrojs/tailwind": "5.1.0",
        "@pagefind/default-ui": "^1.0.4",
        "@tailwindcss/typography": "0.5.10",
        "@tinacms/cli": "^1.5.30",
        "clsx": "2.0.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "mdast-util-to-string": "^4.0.0",
        "motion": "^10.16.4",
        "pagefind": "^1.0.3",
        "reading-time": "^1.5.0",
        "slugify": "^1.6.6",
        "tailwind-merge": "2.0.0",
        "tailwindcss": "3.3.5",
        "tinacms": "^1.5.21",
        "typescript": "^5.2.2"
    }
}
