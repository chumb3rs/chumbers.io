{
    "name": "chumbers.io",
    "type": "module",
    "version": "1.1.0",
    "description": "My personal blog website",
    "author": "chumb3rs",
    "license": "GPL-3.0-only",
    "repository": {
        "type": "git",
        "url": "https://github.com/chumb3rs/chumbers.io"
    },
    "keywords": [
        "astro",
        "astrojs",
        "astro-template",
        "astro-blog",
        "astro-b"
    ],
    "bugs": {
        "url": "https://github.com/chumb3rs/chumbers.io/issues"
    },
    "scripts": {
        "dev": "npx astro dev",
        "clean": "rm -rf .astro && rm -rf node_modules/.astro && rm -rf node_modules/.vite",
        "start": "astro dev",
        "build": "astro build",
        "sync": "astro sync",
        "preview": "astro preview",
        "postbuild": "pagefind --site dist",
        "format:biome": "biome format --write",
        "format:astro": "prettier --write \"**/*.astro\"",
        "format": "npm run format:astro && npm run format:biome",
        "format:check:biome": "biome format --check --format-with-errors=true",
        "format:check:astro": "prettier --check \"**/*.astro\"",
        "format:check": "npm run format:check:biome && npm run format:check:astro",
        "lint": "biome lint --write",
        "lint:fix": "biome check --write",
        "pre-commit": "lint-staged",
        "prepare": "husky"
    },
    "dependencies": {
        "@astrojs/rss": "^4.0.13",
        "@aws-sdk/client-ses": "^3.934.0",
        "astro": "^5.15.4",
        "astro-cloudinary": "^1.3.5",
        "astro-font": "1.1.0",
        "notyf": "^3.10.0",
        "rehype-katex": "^7.0.1",
        "remark-math": "^6.0.0"
    },
    "devDependencies": {
        "@astrojs/mdx": "^4.3.10",
        "@astrojs/sitemap": "^3.6.0",
        "@astrojs/tailwind": "^6.0.2",
        "@biomejs/biome": "2.3.2",
        "@pagefind/default-ui": "^1.0.4",
        "@tailwindcss/typography": "0.5.10",
        "clsx": "2.0.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.0",
        "mdast-util-to-string": "^4.0.0",
        "motion": "^10.16.4",
        "pagefind": "^1.0.3",
        "prettier": "^3.6.2",
        "prettier-plugin-astro": "^0.14.1",
        "reading-time": "^1.5.0",
        "slugify": "^1.6.6",
        "tailwind-merge": "2.0.0",
        "tailwindcss": "3.3.5",
        "typescript": "^5.2.2"
    }
}
